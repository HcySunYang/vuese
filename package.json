{
  "name": "vuese",
  "version": "1.1.1",
  "main": "dist/vuese.js",
  "bin": "bin/cli.js",
  "types": "dist/index.d.ts",
  "unpkg": "dist/vuese.js",
  "jsdelivr": "dist/vuese.js",
  "files": [
    "dist",
    "bin",
    "lib"
  ],
  "keywords": [
    "vue",
    "docs",
    "document",
    "vue-cli"
  ],
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "test": "yarn lint:ts && jest --verbose",
    "build": "yarn lint:ts && bili",
    "postbuild": "find dist -type d -name __test__ -exec rm -r {} +",
    "prepublishOnly": "yarn build",
    "toc": "markdown-toc -i README.md",
    "lint:ts": "tslint --project tsconfig.json --fix",
    "lint:js": "eslint --fix"
  },
  "repository": "https://github.com/HcySunYang/vuese.git",
  "author": "HcySunYang <HcySunYang@outlook.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/generator": "^7.0.0",
    "@babel/parser": "^7.1.0",
    "@babel/traverse": "^7.1.0",
    "@babel/types": "^7.1.3",
    "@vue/component-compiler-utils": "^2.2.0",
    "cac": "^5.0.13",
    "carlo": "^0.9.10",
    "chokidar": "^2.0.4",
    "fast-glob": "^2.2.3",
    "get-port": "^4.0.0",
    "inquirer": "^6.2.0",
    "joycon": "^2.1.2",
    "log-horizon": "^0.1.2",
    "majo": "^0.6.2",
    "marked": "^0.5.1",
    "opn": "^5.4.0",
    "prismjs": "^1.15.0",
    "sao": "^0.22.17",
    "serve-handler": "^5.0.5",
    "vue-template-compiler": "^2.5.17"
  },
  "devDependencies": {
    "@types/babel__generator": "^7.0.0",
    "@types/babel__traverse": "^7.0.0",
    "@types/jest": "^23.3.2",
    "@types/node": "^10.11.0",
    "bili": "^3.1.2",
    "eslint": "^5.8.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-prettier": "^3.0.0",
    "husky": "^1.0.0",
    "jest": "^23.6.0",
    "lint-staged": "^7.3.0",
    "markdown-toc": "^1.2.0",
    "prettier": "^1.14.3",
    "prettier-tslint": "^0.4.0",
    "rollup-plugin-typescript2": "^0.17.0",
    "ts-jest": "^23.10.1",
    "tslint": "^5.11.0",
    "tslint-plugin-prettier": "^2.0.0",
    "typescript": "^3.0.3"
  },
  "bili": {
    "input": "src/index.ts",
    "js": "typescript2",
    "filename": "vuese.js",
    "typescript2": {
      "useTsconfigDeclarationDir": true
    },
    "externals": [
      "@babel/generator",
      "@babel/parser",
      "@babel/traverse",
      "@babel/types",
      "@vue/component-compiler-utils",
      "vue-template-compiler"
    ]
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint:ts --",
      "git add"
    ],
    "bin/**/*.js": [
      "npm run lint:js --",
      "git add"
    ],
    "lib/**/*.js": [
      "npm run lint:js --",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
